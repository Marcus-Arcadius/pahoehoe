<script type="text/javascript">
$(function(){ // on document ready

$('input[id^=tab-input-]').
	each(function(){
		var tab_id = this.id.substr('tab-input-'.length);
		$(this).
			blur(function(){
				firehose_save_tab(tab_id);
			}).
			keypress(function(event){
				var RETURN=13, SPACE=32;

				var $this = $(this);
				switch ( event.which || event.keyCode ) {
					case RETURN:
					case SPACE:
						firehose_save_tab(tab_id);
						return false;
					default:
						return true;
				}
			});
	})

});
</script>
